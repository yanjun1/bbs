<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="cn.edu.qtech.vo.Picture" table="picture">
    <id name="id" column="id" type="long">
      <generator class="increment"/>
    </id>
    <property name="filename" column="filename" type="string"></property>
    <property name="pic" column="pic" type="java.sql.Blob"></property>
	<property name="createTime" column="create_time" type="java.util.Date"></property>
  </class>
</hibernate-mapping>
